<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.seeyon.v3x.hr.domain.Page" table="hr_pages">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property
          name="pageName"
          type="string"
          column="page_name"
          length="255"
          not-null="true"
       />
       <property
          name="pageNo"
          type="int"
          column="page_no"
          length="4"
          not-null="true"
       />
       <property
          name="pageDisplay"
          type="int"
          column="page_display"
          length="4"
          not-null="true"
       />
       <property
          name="repair"
          type="int"
          column="repair"
          length="4"
          not-null="false"
       />
       <property
          name="modelName"
          type="string"
          column="model_name"
          length="255"
          not-null="true"
       />
       <property
          name="memo"
          type="clob"
          column="memo"
          not-null="false" 
          />
        <property
          name="remove"
          type="int"
          column="remove"
          length="4"
          not-null="true"
       />
        <property
          name="accountId"
          type="long"
          column="accountId"
          length="20"
       />
       <property
          name="sort"
          type="int"
          column="sort"
          length="4"
          not-null="true"
       />
       <property
			name="sysFlag"
			column="sys_flag"
			type="boolean"
			not-null="true"
			length="1"
		/>
	</class>
</hibernate-mapping>