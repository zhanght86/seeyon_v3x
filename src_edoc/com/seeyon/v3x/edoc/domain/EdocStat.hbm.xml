<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.seeyon.v3x.edoc.domain.EdocStat" lazy="false" table="edoc_stat">
	<id
        name="id"
        type="long"
        column="id"
        length="20"
    >
		<generator class="assigned" />
	</id>
	<property
        name="edocId"
        type="long"
        column="edoc_id"
		not-null="true"
        length="20"
    />
	<property
        name="edocType"
        type="integer"
        column="edoc_type"
		not-null="true"
        length="4"
    />
	<property
        name="subject"
        type="string"
        column="subject"
        not-null="true"
        length="1000"
    />
    <property
        name="docType"
        type="string"
        column="doc_type"
        length="255"
    />
    <property
        name="year"
        type="string"
        column="year"
        length="4"
    />
    <property
        name="month"
        type="string"
        column="month"
        length="2"
    />        
	<property
        name="docMark"
        type="string"
        column="doc_mark"
        length="255"
    />
	<property
        name="deptId"
        type="long"
        column="dept_id"
        length="20"
    />    
	<property
        name="createDate"
        type="timestamp"
        column="create_date"
        length="19"
    />
	<property
        name="isSent"
        type="boolean"
        column="is_sent"
        length="4"
    />
	<property
        name="isArchived"
        type="boolean"
        column="is_archived"
		not-null="true"
        length="4"
    />
    <property
        name="flowState"
        type="integer"
        column="flow_state"
        not-null="true"
        length="4"
    />
     <property
        name="domainId"
        type="long"
        column="domain_id"
        not-null="true"
        length="20"
    />
    <property
        name="remark"
        type="string"
        column="remark"
        length="1000"
    />
    <property
        name="sendTo"
        type="string"
        column="send_to"
        length="255"
    />
	<property
        name="copyTo"
        type="string"
        column="copy_to"
        length="255"
    />
    <property
        name="issuer"
        type="string"
        column="issuer"
        length="255"
    />
    <property
        name="copies"
        type="integer"
        column="copies"
        length="11"
    />
    <property
        name="contentNo"
        type="integer"
        column="content_no"
        length="4"
    />
    
    <property name="serialNo"
              type="string"
              column="serial_no"
              length="255"/>
     <property name="archivedTime"
               type="timestamp"
               column="archived_time"
               length="19"/>
     <property name="accountId"
               type="java.lang.Long" 
               column="account_id"
               length="20"/>
     <property name="createUserid"
               type="java.lang.Long" 
               column="create_user_id"
               length="20"/>                         

    
</class>
</hibernate-mapping>
